# 0 "arch/arm64/boot/dts/overlays/pciex1-compat-pi5-overlay.dts"
# 0 "<built-in>"
# 0 "<command-line>"
# 1 "arch/arm64/boot/dts/overlays/pciex1-compat-pi5-overlay.dts"




/dts-v1/;
/plugin/;

/ {
 compatible = "brcm,bcm2712";


 fragment@0 {
  target = <&pciex1>;
  __dormant__ {
   brcm,enable-l1ss;
  };
 };


 fragment@1 {
  target = <&pciex1>;
  __dormant__ {
   aspm-no-l0s;
  };
 };


 fragment@2 {
  target = <&pciex1>;
  __dormant__ {
   msi-parent = <&pciex1>;
  };
 };

 __overrides__ {
  l1ss = <0>, "+0";
  no-l0s = <0>, "+1";
  no-mip = <0>, "+2";
 };
};
