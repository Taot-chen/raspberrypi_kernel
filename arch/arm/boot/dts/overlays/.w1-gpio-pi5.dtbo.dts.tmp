# 0 "arch/arm64/boot/dts/overlays/w1-gpio-pi5-overlay.dts"
# 0 "<built-in>"
# 0 "<command-line>"
# 1 "arch/arm64/boot/dts/overlays/w1-gpio-pi5-overlay.dts"
/dts-v1/;
/plugin/;

# 1 "arch/arm64/boot/dts/overlays/w1-gpio-overlay.dts" 1

/dts-v1/;
/plugin/;

/ {
 compatible = "brcm,bcm2835";

 fragment@0 {
  target-path = "/";
  __overlay__ {

   w1: onewire@0 {
    compatible = "w1-gpio";
    pinctrl-names = "default";
    pinctrl-0 = <&w1_pins>;
    gpios = <&gpio 4 0>;
    status = "okay";
   };
  };
 };

 fragment@1 {
  target = <&gpio>;
  __overlay__ {
   w1_pins: w1_pins@0 {
    brcm,pins = <4>;
    brcm,function = <0>;
    brcm,pull = <0>;
   };
  };
 };

 __overrides__ {
  gpiopin = <&w1>,"gpios:4",
    <&w1>,"reg:0",
    <&w1_pins>,"brcm,pins:0",
    <&w1_pins>,"reg:0";
  pullup;
 };
};
# 5 "arch/arm64/boot/dts/overlays/w1-gpio-pi5-overlay.dts" 2

/ {
 compatible = "brcm,bcm2712";

 fragment@2 {
  target = <&w1>;
  __overlay__ {
   raspberrypi,delay-needs-poll;
  };
 };
};
